create table MOCK_DATA (
	id VARCHAR(40),
	poster_id VARCHAR(40),
	book_author VARCHAR(50),
	book_name TEXT,
	isbn VARCHAR(50),
	pages INT,
	likes INT,
	poster_friends INT,
	poster_name VARCHAR(50),
	poster_email VARCHAR(50),
	poster_job VARCHAR(50)
);
